        "# @title 3. Arquitetura: Defini√ß√£o das Estruturas e Agentes\n",
        "# Descri√ß√£o: Esta c√©lula cont√©m o n√∫cleo intelectual do nosso sistema.\n",
        "# Definimos a estrutura de dados que o LLM deve gerar (ApolloQuery) e\n",
        "# implementamos as classes para cada um dos cinco agentes especializados\n",
        "# que comp√µem nosso pipeline A2A (Agent-to-Agent).\n",
        "\n",
        "# --- Estrutura de Dados para o LLM ---\n",
        "class ApolloQuery(BaseModel):\n",
        "    \"\"\"Define o formato JSON esperado pela API do Apollo.io para a busca de pessoas.\"\"\"\n",
        "    q_person_titles: List[str] = Field(default_factory=list, description=\"Lista de cargos. Ex: ['Marketing Manager']\")\n",
        "    person_locations: List[str] = Field(default_factory=list, description=\"Lista de localiza√ß√µes. Ex: ['Brazil']\")\n",
        "    organization_num_employees_ranges: List[str] = Field(default_factory=list, description=\"Faixas de tamanho da empresa. Ex: ['11,50']\")\n",
        "    q_organization_keyword_tags: List[str] = Field(default_factory=list, description=\"Palavras-chave da organiza√ß√£o. Ex: ['education']\")\n",
        "\n",
        "# --- Implementa√ß√£o dos Agentes ---\n",
        "class InputAgent:\n",
        "    \"\"\"Agente 1: Define os crit√©rios de busca.\"\"\"\n",
        "    def get_criteria(self) -> str:\n",
        "        print(\"üïµÔ∏è Agente 1: Definindo crit√©rios de busca de leads.\")\n",
        "        return \"Preciso de leads que trabalham com marketing ou s√£o gerentes de marketing, localizados no Brasil, em empresas de educa√ß√£o ou edtech com 11 a 50 funcion√°rios.\"\n",
        "\n",
        "class QueryGenerationAgent:\n",
        "    \"\"\"Agente 2: Usa o Gemini para traduzir os crit√©rios em um payload de API.\"\"\"\n",
        "    def __init__(self, api_key: str):\n",
        "        llm = ChatGoogleGenerativeAI(model=\"gemini-2.5-flash\", google_api_key=api_key, temperature=0)\n",
        "        structured_llm = llm.with_structured_output(ApolloQuery)\n",
        "        prompt = ChatPromptTemplate.from_messages([\n",
        "            (\"system\", \"Voc√™ √© um especialista em gerar payloads para a API do Apollo.io. Converta a solicita√ß√£o do usu√°rio em um JSON estruturado para o endpoint /v1/people/search, mapeando os crit√©rios para os campos corretos. Retorne apenas o objeto JSON.\"),\n",
        "            (\"human\", \"{user_request}\")\n",
        "        ])\n",
        "        self.chain = prompt | structured_llm\n",
        "\n",
        "    def generate_query(self, criteria: str) -> Dict[str, Any]:\n",
        "        print(\"ü§ñ Agente 2: Gerando payload para a API Apollo com o Gemini...\")\n",
        "        query_payload = self.chain.invoke({\"user_request\": criteria})\n",
        "        print(\"‚úÖ Payload gerado:\", query_payload.dict())\n",
        "        return query_payload.dict()\n",
        "\n",
        "class ApolloAPIAgent:\n",
        "    \"\"\"Agente 3: Interage com a API do Apollo.io e lida com a pagina√ß√£o.\"\"\"\n",
        "    BASE_URL = \"https://api.apollo.io/v1/people/search\"\n",
        "\n",
        "    def __init__(self, api_key: str):\n",
        "        self.api_key = api_key\n",
        "\n",
        "    def fetch_leads(self, query_payload: Dict[str, Any], max_leads: int = 500) -> List[Dict[str, Any]]:\n",
        "        all_leads, page, per_page = [], 1, 100\n",
        "        # Define os cabe√ßalhos da requisi√ß√£o, incluindo a chave da API\n",
        "        headers = {\n",
        "            \"X-Api-Key\": self.api_key,\n",
        "            \"Content-Type\": \"application/json\"\n",
        "        }\n",
        "        print(f\"\\nüìû Agente 3: Iniciando busca de at√© {max_leads} leads na API do Apollo...\")\n",
        "        while len(all_leads) < max_leads:\n",
        "            payload = {\"api_key\": self.api_key, \"page\": page, \"per_page\": per_page, **query_payload}\n",
        "            try:\n",
        "                with httpx.Client() as client:\n",
        "                    response = client.post(self.BASE_URL, json=payload, headers=headers, timeout=30.0)\n",
        "                    response.raise_for_status()\n",
        "                data = response.json()\n",
        "                leads_on_page = data.get(\"people\", [])\n",
        "                if not leads_on_page:\n",
        "                    print(\"üèÅ N√£o foram encontrados mais leads. Fim da busca.\")\n",
        "                    break\n",
        "                all_leads.extend(leads_on_page)\n",
        "                print(f\"üìÑ P√°gina {page}: {len(leads_on_page)} leads encontrados. Total: {len(all_leads)}\")\n",
        "                page += 1\n",
        "                time.sleep(1)\n",
        "            except httpx.HTTPStatusError as e:\n",
        "                print(f\"‚ùå Erro na API do Apollo: {e.response.status_code} - {e.response.text}\")\n",
        "                break\n",
        "            except Exception as e:\n",
        "                print(f\"‚ùå Ocorreu um erro inesperado: {e}\")\n",
        "                break\n",
        "        return all_leads[:max_leads]\n",
        "\n",
        "class DeduplicationAgent:\n",
        "    \"\"\"Agente 4: Remove duplicatas da lista de leads.\"\"\"\n",
        "    def remove_duplicates(self, leads: List[Dict[str, Any]]) -> List[Dict[str, Any]]:\n",
        "        print(\"\\nüßº Agente 4: Removendo leads duplicados com base no email...\")\n",
        "        unique_leads, seen_emails = [], set()\n",
        "        for lead in leads:\n",
        "            email = lead.get(\"email\")\n",
        "            if email and email not in seen_emails:\n",
        "                unique_leads.append(lead)\n",
        "                seen_emails.add(email)\n",
        "        print(f\"‚ú® Leads √∫nicos: {len(unique_leads)} (de {len(leads)} brutos)\")\n",
        "        return unique_leads\n",
        "\n",
        "class CSVExportAgent:\n",
        "    \"\"\"Agente 5: Exporta os leads √∫nicos para um arquivo CSV.\"\"\"\n",
        "    def export(self, leads: List[Dict[str, Any]], filename: str = \"leads_apollo.csv\"):\n",
        "        print(f\"\\nüíæ Agente 5: Exportando leads para o arquivo '{filename}'...\")\n",
        "        if not leads:\n",
        "            print(\"‚ö†Ô∏è Nenhum lead para exportar.\")\n",
        "            return\n",
        "        relevant_data = [{\n",
        "            \"nome_completo\": l.get(\"name\"), \"primeiro_nome\": l.get(\"first_name\"),\n",
        "            \"ultimo_nome\": l.get(\"last_name\"), \"email\": l.get(\"email\"),\n",
        "            \"cargo\": l.get(\"title\"), \"empresa\": l.get(\"organization\", {}).get(\"name\"),\n",
        "            \"url_linkedin\": l.get(\"linkedin_url\"), \"localizacao\": l.get(\"headline\"),\n",
        "        } for l in leads]\n",
        "        df = pd.DataFrame(relevant_data)\n",
        "        df.to_csv(filename, index=False, encoding='utf-8')\n",
        "        print(f\"‚úÖ Arquivo '{filename}' salvo com {len(df)} leads. Verifique no painel de arquivos.\")\n",
        "\n",
        "print(\"Defini√ß√µes dos Agentes carregadas com sucesso.\")"
      ],
